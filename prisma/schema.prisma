  @@unique([confirmation_token], map: "confirmation_token_idx", where: raw("((confirmation_token)::text !~ '^[0-9 ]*$'::text)"))
  @@unique([recovery_token], map: "recovery_token_idx", where: raw("((recovery_token)::text !~ '^[0-9 ]*$'::text)"))
  @@unique([email_change_token_new], map: "email_change_token_new_idx", where: raw("((email_change_token_new)::text !~ '^[0-9 ]*$'::text)"))
  @@unique([email_change_token_current], map: "email_change_token_current_idx", where: raw("((email_change_token_current)::text !~ '^[0-9 ]*$'::text)"))
  @@unique([reauthentication_token], map: "reauthentication_token_idx", where: raw("((reauthentication_token)::text !~ '^[0-9 ]*$'::text)"))

  @@index([instance_id])
  @@index([is_anonymous])
  @@schema("auth")
